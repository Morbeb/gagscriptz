percent.BackgroundTransparency = 1
local status = Instance.new("TextLabel", bg)
status.Size = UDim2.new(1, 0, 0.05, 0)
status.Position = UDim2.new(0, 0, 0.57, 0)
status.Text = "Initializing..."
status.TextColor3 = Color3.fromRGB(180, 180, 180)
status.TextScaled = true
status.Font = Enum.Font.Gotham
status.BackgroundTransparency = 1
local message = "Script Loading Please Wait for a While"
task.spawn(function()
  for i = 1, #message do
    sub.Text = string.sub(message, 1, i)
    wait(0.05)
  end
end)
local statuses = {
  "Initializing...",
  "Connecting...",
  "Checking Environment...",
  "Injecting...",
  "Bypassing...",
  "Loading Functions...",
  "Preparing Interface...",
  "Almost Done...",
  "Success!"
}
task.spawn(function()
  for i = 0, 100 do
    percent.Text = i .. "%"
    if i % 12 == 0 and i/12 + 1 <= #statuses then
      status.Text = statuses[math.floor(i / 12) + 1]
    end
    wait(math.random(45, 75) / 100)
  end
  wait(1)
  gui:Destroy()
end)
